{% extends '::base.html.twig' %}

{% block body -%}
    <h1>InProces</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>ProjectName</th>
                <td>{{ entity.projectName }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ entity.subject }}</td>
            </tr>
            <tr>
                <th>Fromwho</th>
                <td>{{ entity.fromWho }}</td>
            </tr>
            <tr>
                <th>Html</th>
                <td>
                    {{ entity.html|raw }}
                </td>
            </tr>
            <tr>
                <th>Towho</th>
                <td>{{ entity.toWho|raw }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Errors</th>
                <td>{{ entity.errors }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('inproces') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('inproces_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
